<%= form_for @user do |f| %>
  <%= f.label :username, "You username here" %>:
  <%= f.text_field :username %><br/>
  <%= f.label :email%>:
  <%= f.email_field :email, placeholder: "example@example.com" %><br/>
  <%= f.label :password %>:
  <%= f.password_field :password %><br/>
  <%= f.submit "Sign Up"%>
<% end %>

